                        android:text="@string/how_setting"
                        android:textColor="@color/colorAccent"
                        android:textSize="@dimen/text_size_14" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <android.support.v7.widget.RecyclerView
                        android:id="@+id/qingjia_csr_recyclerview"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/margin90"
                        android:background="@color/white"
                        android:nestedScrollingEnabled="false" />

                    <ImageView
                        android:id="@+id/qingjia_csr_img"
                        android:layout_width="@dimen/margin50"
                        android:layout_height="@dimen/margin50"
                        android:layout_marginLeft="@dimen/margin15"
                        android:src="@mipmap/add_spr_img_ioc" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:orientation="vertical">


                    <Button
                        android:id="@+id/qingjia_save_btn"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:layout_marginBottom="@dimen/margin50"
                        android:layout_marginLeft="@dimen/margin15"
                        android:layout_marginRight="@dimen/margin15"
                        android:layout_marginTop="50dp"
                        android:background="@drawable/save_btn_style"
                        android:text="@string/submit_txt"
                        android:textColor="@color/white"
                        android:textSize="18sp" />

                </LinearLayout>


            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
