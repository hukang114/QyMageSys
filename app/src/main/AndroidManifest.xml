        <activity android:name=".ui.act.LoanApplicationActivity" />
        <activity android:name=".ui.act.ApplicationRepaymentActivity" />
        <activity android:name=".ui.act.ProjectApprovaApplylActivity" />
        <activity android:name=".ui.act.PerformanceBondZhiActivity" />
        <activity android:name=".ui.act.PerformanceBondShouActivity" />
        <activity android:name=".ui.act.BiddingMarginZhiActivity" />
        <activity android:name=".ui.act.BiddingMarginShouActivity" />
        <activity android:name=".ui.act.ContractApplicationActivity" />
        <activity android:name=".ui.act.ContractDetailsAddActivity" />
        <activity android:name=".ui.act.ContractPaymentRaActivity" />
        <activity android:name=".ui.act.ReceiverInfoActivity" />
        <activity android:name=".ui.act.AskForLeaveActivity" />
        <activity android:name=".ui.act.ReceiptsPaymentsDetActivity"></activity>
        <activity android:name=".ui.act.AskForLeaveRecordlActivity"></activity>
    </application>

</manifest>